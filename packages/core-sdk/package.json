{
  "name": "mtg-card-analyzer-sdk",
  "version": "0.0.5-alpha.0",
  "description": "a mtg ocr app for documenting a collection",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run compile",
    "coverage": "nyc -r lcov -e .js -x \"*.spec.js\" npm run test",
    "coverage:ts": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "coverage:js": "nyc -r lcov -e .js -x \"*.spec.js\" npm run test",
    "compile": "tsc",
    "clean": "tsc --build --clean",
    "test": "npm run test:js && npm run test:ts",
    "prepare": "npm run build",
    "test:js": "mocha \"./test/**/*.spec.js\"",
    "test:ts": "mocha -r ts-node/register test/**/*.spec.ts",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dills122/mtg-processor.git"
  },
  "keywords": [
    "mtg",
    "ocr",
    "collector"
  ],
  "author": "Dylan Steele",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "async": "^3.1.0",
    "bunyan": "^1.8.12",
    "clean-text-utils": "^1.1.17",
    "fuzzyset.js": "0.0.8",
    "image-hash": "^4.0.1",
    "image-size": "^0.9.1",
    "image-to-base64": "^2.1.1",
    "jimp": "^0.16.1",
    "lodash": "^4.17.19",
    "meow": "^5.0.0",
    "mysql": "^2.18.1",
    "nedb": "^1.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "string-similarity": "^3.0.0",
    "temp-dir": "^2.0.0",
    "tesseract.js": "^2.1.1",
    "tmp": "^0.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@hapi/joi": "^15.0.3",
    "@types/assert": "^1.5.1",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.12",
    "@types/fuzzyset.js": "0.0.2",
    "@types/hapi__joi": "^17.1.4",
    "@types/image-size": "^0.8.0",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^8.0.0",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.0.26",
    "@types/proxyquire": "^1.3.28",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@types/sinon": "^9.0.4",
    "@types/tesseract.js": "0.0.2",
    "@types/uuid": "^8.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
