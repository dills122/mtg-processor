    "clean-text-utils": "^1.1.17",
    "fuzzyset.js": "0.0.8",
    "image-hash": "^3.5.1",
    "image-size": "^0.7.3",
    "image-to-base64": "^2.0.1",
    "jimp": "^0.6.4",
    "lodash": "^4.17.15",
    "meow": "^5.0.0",
    "mysql": "^2.17.1",
    "nedb": "^1.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "string-similarity": "^3.0.0",
    "temp-dir": "^2.0.0",
    "tesseract.js": "^1.0.19",
    "tmp": "^0.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
