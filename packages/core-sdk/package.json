  "description": "a mtg ocr app for documenting a collection",
  "main": "dist/index.js",
  "scripts": {
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "compile": "tsc",
    "test": "mocha \"./test/**/*.spec.js\"",
    "test:js": "mocha \"./test/**/*.spec.js\"",
    "test:ts": "mocha -r ts-node/register tests/**/*.test.ts",
