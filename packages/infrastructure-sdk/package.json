{
  "name": "infrastructure-sdk",
  "version": "0.0.3-alpha.0",
  "bin": {
    "single-processor": "bin/infrastructure-sdk.js"
  },
  "description": "infrastructure code for mtg processor",
  "keywords": [
    "mtg",
    "aws",
    "cdk"
  ],
  "author": "dills122 <dylansteele57@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "main": "lib/infrastructure-sdk.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "coverage": "nyc -r lcov -e .js -x \"*.spec.js\" npm run test",
    "coverage:ts": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "coverage:js": "nyc -r lcov -e .js -x \"*.spec.js\" npm run test",
    "compile": "tsc",
    "clean": "tsc --build --clean",
    "clean:cdk": "rm -rf ./cdk.out",
    "test": "mocha \"./test/**/*.spec.js\"",
    "test:js": "mocha \"./test/**/*.spec.js\"",
    "test:ts": "mocha -r ts-node/register tests/**/*.spec.ts",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.54.0",
    "@aws-cdk/aws-ec2": "^1.54.0",
    "@aws-cdk/aws-efs": "^1.54.0",
    "@aws-cdk/aws-lambda": "^1.54.0",
    "@aws-cdk/core": "^1.54.0",
    "aws-cdk": "^1.54.0",
    "aws-sdk": "^2.721.0",
    "eslint": "^7.5.0",
    "mtg-card-analyzer-sdk": "file:../core-sdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/node": "^14.0.26",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
